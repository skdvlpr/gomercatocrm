{
  "fields": {
    "name": {
      "type": "varchar",
      "required": true,
      "maxLength": 150
    },
    "csvUrl": {
      "type": "url",
      "required": true,
      "tooltip": true
    },
    "isActive": {
      "type": "bool",
      "default": false
    },
    "lastProcessedRow": {
      "type": "int",
      "default": 0,
      "readOnly": true
    },
    "lastRunAt": {
      "type": "datetime",
      "readOnly": true
    },
    "lastError": {
      "type": "text",
      "readOnly": true,
      "maxLength": 1000
    },
    "leadsImportedCount": {
      "type": "int",
      "default": 0,
      "readOnly": true
    },
    "firstRowIsHeader": {
      "type": "bool",
      "default": true
    },
    "fieldMapping": {
      "type": "jsonObject",
      "tooltip": true
    },
    "defaultFieldMapping": {
      "type": "bool",
      "default": true,
      "tooltip": true
    },
    "assignedUser": {
      "type": "link"
    },
    "team": {
      "type": "link"
    },
    "leadSource": {
      "type": "enum",
      "customizationOptionsDisabled": true,
      "optionsPath": "entityDefs.Lead.fields.source.options",
      "translation": "Lead.options.source",
      "default": "Web Site"
    },
    "skipDuplicates": {
      "type": "bool",
      "default": true,
      "tooltip": true
    },
    "duplicateCheckFields": {
      "type": "multiEnum",
      "options": ["emailAddress", "phoneNumber", "name"],
      "default": ["emailAddress"]
    },
    "description": {
      "type": "text"
    },
    "createdAt": {
      "type": "datetime",
      "readOnly": true
    },
    "modifiedAt": {
      "type": "datetime",
      "readOnly": true
    },
    "createdBy": {
      "type": "link",
      "readOnly": true
    },
    "modifiedBy": {
      "type": "link",
      "readOnly": true
    }
  },
  "links": {
    "assignedUser": {
      "type": "belongsTo",
      "entity": "User"
    },
    "team": {
      "type": "belongsTo",
      "entity": "Team"
    },
    "createdBy": {
      "type": "belongsTo",
      "entity": "User"
    },
    "modifiedBy": {
      "type": "belongsTo",
      "entity": "User"
    }
  },
  "collection": {
    "orderBy": "createdAt",
    "order": "desc"
  }
}
